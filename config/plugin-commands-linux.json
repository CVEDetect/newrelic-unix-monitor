		{
			"eventType": "Process",
			"command": "ps -ewwo pid,state,%cpu,%mem,rss,vsz,command",
			"checkAllRegex": false,
			"lineLimit": 0,
			"mappings": [{
				"expression": "(\\d+)\\s+([A-Z]{1})\\s+([0-9\\.]+)\\s+([0-9\\.]+)\\s+(\\d+)\\s+(\\d+)\\s+(.*)",
				"metrics": [{
						"name": "PID",
						"type": "ATTRIBUTE"
					},
					{
						"name": "State",
						"type": "ATTRIBUTE"
					},
					{
						"name": "CPU",
						"ratio": 1,
						"type": "NORMAL"
					},
					{
						"name": "Memory",
						"ratio": 1,
						"type": "NORMAL"
					},
					{
						"name": "Resident Set Size",
						"ratio": 1,
						"type": "NORMAL"
					},
					{
						"name": "Virtual Size",
						"ratio": 1,
						"type": "NORMAL"
					},
					{
						"name": "PROCESS_NAME"
					}
				]
			}]
		},
