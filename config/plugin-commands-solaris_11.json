{
	"os_settings": {
		"disksCommand": "",
		"disksRegex": "",
		"interfacesCommand": "/usr/sbin/ifconfig -a",
		"interfacesRegex": "(\\w+\\d*):\\d*:*\\s+flags.*",
		"pageSizeCommand": "pagesize"
	},
	"commands": [{
			"eventType": "Disk",
			"command": "df -k -l",
			"mappings": [{
				"expression": "\\s*(\\S+)\\s+(\\d+)\\s+(\\d+)\\s+(\\d+)\\s+(\\d+)%\\s+(\\S+).*",
				"metrics": [{
						"name": "METRIC_INSTANCE"
					},
					{
						"name": "Total",
						"type": "NORMAL"
					},
					{
						"name": "Used",
						"type": "NORMAL"
					},
					{
						"name": "Free",
						"type": "NORMAL"
					},
					{
						"name": "Percent Used",
						"type": "NORMAL"
					},
                    {
						"name": "Mounted On",
						"type": "NORMAL"
					}
				]
			}]
		},
		{
			"eventType": "DiskIO",
			"command": "iostat -x -I",
			"mappings": [{
				"expression": "\\s*([\\/\\w\\d]+)\\s+([0-9\\.]+)\\s+([0-9\\.]+)\\s+([0-9\\.]+)\\s+([0-9\\.]+)\\s+([0-9\\.]+)\\s+([0-9\\.]+)\\s+([0-9\\.]+)\\s+(\\d+)\\s+(\\d+)",
				"metrics": [{
						"name": "METRIC_INSTANCE"
					},
					{
						"name": "Reads per Interval",
						"type": "DELTA"
					},
					{
						"name": "Writes per Interval",
						"type": "DELTA"
					},
					{
						"name": "Data Read per Interval",
						"type": "DELTA"
					},
					{
						"name": "Data Written per Interval",
						"type": "DELTA"
					},
					{
						"name": "Average queue length",
						"type": "NORMAL"
					},
					{
						"name": "Active transactions",
						"type": "NORMAL"
					},
					{
						"name": "Average service time",
						"type": "NORMAL"
					},
					{
						"name": "Percentage of Time Non-Empty Queue",
						"type": "NORMAL"
					},
					{
						"name": "Percentage of Time Busy",
						"type": "NORMAL"
					}
				]
			}]
		},
		{
			"eventType": "NetworkIO",
			"command": "kstat -n MEMBER_PLACEHOLDER",
			"mappings": [{
				"expression": "(\\w+\\d*)\\s+([0-9\\.]+)",
				"metrics": [{
						"name": "METRIC_NAME"
					},
					{
						"name": "METRIC_VALUE"
					}
				],
				"translations": [{
						"input": "brdcstrcv",
						"output": "Receive.Broadcast",
						"type": "DELTA"
					},
					{
						"input": "brdcstxmt",
						"output": "Transmit.Broadcast",
						"type": "DELTA"
					},
					{
						"input": "collisions",
						"output": "Collisions",
						"type": "DELTA"
					},
					{
						"input": "ierrors",
						"output": "Receive.Errors",
						"type": "DELTA"
					},
					{
						"input": "ipackets64",
						"output": "Receive.Packets",
						"type": "DELTA"
					},
					{
						"input": "multircv",
						"output": "Receive.Multicast",
						"type": "DELTA"
					},
					{
						"input": "multixmt",
						"output": "Transmit.Multicast",
						"type": "DELTA"
					},
					{
						"input": "norcvbuf",
						"output": "Receive.Buffer Allocation Errors",
						"type": "DELTA"
					},
					{
						"input": "noxmtbuf",
						"output": "Transmit.Buffer Allocation Errors",
						"type": "DELTA"
					},
					{
						"input": "obytes64",
						"output": "Transmit.Bytes",
						"type": "DELTA"
					},
					{
						"input": "oerrors",
						"output": "Transmit.Errors",
						"type": "DELTA"
					},
					{
						"input": "opackets64",
						"output": "Transmit.Packets",
						"type": "DELTA"
					},
					{
						"input": "rbytes64",
						"output": "Receive.Bytes",
						"type": "DELTA"
					}
				]
			}]
		},
		{
			"eventType": "Process",
			"command": "ps -eo pid,s,pcpu,pmem,rss,vsz,user,time,nlwp,args",
			"mappings": [{
				"expression": "(\\d+)\\s+([A-Z]{1})\\s+([0-9\\.]+)\\s+([0-9\\.]+)\\s+(\\d+)\\s+(\\d+)\\s+([A-Za-z0-9]+)\\s+([0-9:]+)\\s+(\\d+)\\s+(.+)",
				"metrics": [{
						"name": "PID",
						"type": "ATTRIBUTE"
					},
					{
						"name": "State",
						"type": "ATTRIBUTE"
					},
					{
						"name": "CPU",
						"type": "NORMAL"
					},
					{
						"name": "Memory",
						"type": "NORMAL"
					},
					{
						"name": "Resident Set Size",
						"type": "NORMAL"
					},
					{
						"name": "Virtual Size",
						"type": "NORMAL"
					},
					{
						"name": "User",
						"type": "NORMAL"
					},
					{
						"name": "Time",
						"type": "NORMAL"
					},
					{
						"name": "Nlwp",
						"type": "NORMAL"
					},
					{
						"name": "PROCESS_NAME"
					}
				]
			}]
		},
		{
			"eventType": "Stats",
			"command": "kstat -p unix::system_pages",
			"mappings": [{
				"expression": "unix:0:system_pages:([\\w_]+)\\s+([0-9\\.]+)",
				"metrics": [{
						"name": "METRIC_NAME"
					},
					{
						"name": "METRIC_VALUE"
					}
				],
				"translations": [{
						"input": "availrmem",
						"output": "Page.Available",
						"type": "DELTA"
					},
					{
						"input": "pagesfree",
						"output": "Page.Free",
						"type": "DELTA"
					},
					{
						"input": "pageslocked",
						"output": "Page.Locked",
						"type": "DELTA"
					},
					{
						"input": "pagestotal",
						"output": "Page.Total",
						"type": "DELTA"
					},
					{
						"input": "pp_kernel",
						"output": "Page.Used By Kernel",
						"type": "DELTA"
					},
					{
						"input": "physmem",
						"output": "Memory.Total",
						"ratio": "PAGE_SIZE/1000",
						"type": "NORMAL"
					},
					{
						"input": "freemem",
						"output": "Memory.Free",
						"ratio": "PAGE_SIZE/1000",
						"type": "NORMAL"
					}
				]
			}]
		},
		{
			"eventType": "Stats",
			"command": "/usr/sbin/swap -s",
			"mappings": [{
				"expression": "total:\\s+(\\d+)k\\s+bytes allocated\\s+\\+\\s+(\\d+)k\\s+reserved\\s+=\\s+(\\d+)k\\s+used,\\s+(\\d+)k\\s+available",
				"metrics": [{
						"name": "Swap.Allocated",
						"type": "NORMAL"
					},
					{
						"name": "Swap.Reserved",
						"type": "NORMAL"
					},
					{
						"name": "Swap.Used",
						"type": "NORMAL"
					},
					{
						"name": "Swap.Available",
						"type": "NORMAL"
					}
				]
			}]
		},
		{
			"eventType": "Stats",
			"command": "top -b",
			"lineLimit": 5,
			"mappings": [{
					"expression": "([0-9\\.]+)\\s+processes:\\s+([0-9]+)\\s+sleeping,\\s+([0-9]+) on cpu",
					"metrics": [{
							"name": "Processes.Total",
							"type": "NORMAL"
						},

						{
							"name": "Processes.Sleeping",
							"type": "NORMAL"
						},

						{
							"name": "Processes.Running",
							"type": "NORMAL"
						}
					]
				},

				{
					"expression": "CPU states:\\s+([0-9\\.]+)%\\s+idle,\\s+([0-9\\.]+)%\\s+user,\\s+([0-9\\.]+)%\\s+kernel,\\s+([0-9\\.]+)%\\s+stolen,\\s+([0-9\\.]+)%\\s+swap",
					"metrics": [{
							"name": "CPU.Idle",
							"type": "NORMAL"
						},

						{
							"name": "CPU.User",
							"type": "NORMAL"
						},

						{
							"name": "CPU.Kernel",
							"type": "NORMAL"
						},

						{
							"name": "CPU.Stolen",
							"type": "NORMAL"
						},

						{
							"name": "CPU.Swap",
							"type": "NORMAL"
						}
					]
				},

				{
					"expression": "Memory:\\s+([0-9]+)M\\s+phys mem,\\s+([0-9]+)M\\s+free mem,\\s+([0-9]+)M\\s+total swap,\\s+([0-9]+)M\\s+free swap",
					"metrics": [{
							"name": "PhysicalMemory.Total",
							"ratio": 1024,
							"type": "NORMAL"
						},

						{
							"name": "PhysicalMemory.Free",
							"ratio": 1024,
							"type": "NORMAL"
						},

						{
							"name": "Swap.Total",
							"ratio": 1024,
							"type": "NORMAL"
						},

						{
							"name": "Swap.Free",
							"ratio": 1024,
							"type": "NORMAL"
						}
					]
				},

				{
					"expression": "load averages:\\s+([0-9\\.]+),\\s+([0-9\\.]+),\\s+([0-9\\.]+);.*",
					"metrics": [{
							"name": "LoadAverage.1 Minute",
							"type": "NORMAL"
						},

						{
							"name": "LoadAverage.5 Minute",
							"type": "NORMAL"
						},

						{
							"name": "LoadAverage.15 Minute",
							"type": "NORMAL"
						}
					]
				}
			]
		},
		{
			"eventType": "Stats",
			"command": "vmstat 1 2",
			"mappings": [{
				"expression": "\\s*(\\d+)\\s+(\\d+)\\s+(\\d+)\\s+(\\d+).*",
				"metrics": [{
						"name": "KernelThreads.Runnable",
						"type": "NORMAL"
					},
					{
						"name": "KernelThreads.In Wait Queue",
						"type": "NORMAL"
					},
					{
						"name": "KernelThreads.Swapped",
						"type": "NORMAL"
					},
					{
						"name": "Memory.Swap",
						"type": "NORMAL"
					}
				]
			}]
		},
		{
			"eventType": "Vmstat",
			"command": "vmstat -s",
			"mappings": [{
				"expression": "\\s*(\\d+)\\s+(.+)",
				"metrics": [{
						"name": "METRIC_VALUE",
						"type": "DELTA"
					},
					{
						"name": "METRIC_NAME"
					}
				],
				"translations": [{
					"input": "total address trans. faults taken",
					"output": "translation faults",
					"type": "DELTA"
				}]
			}]
		},
{
      "eventType": "Memstat",
      "command": "echo ::memstat | mdb -k",
      "mappings": [
        {
          "expression": "^([\\w_ /\\(\\)]+)\\s+([0-9\\.]+)\\s+.*",
          "metrics": [
            {
              "name": "METRIC_INSTANCE"
            },
            {
              "name": "Page Memory",
              "ratio": "PAGE_SIZE/1000",
              "type": "NORMAL"
            }
          ]
        }
      ]
    },
{
      "eventType": "ServiceStatus",
      "command": "svcs -av",
      "mappings": [
        {
          "expression": "([a-zA-Z_]+)\\s+([a-zA-Z-]+)\\s+(.*)\\s+([0-9_-]+)\\s+(.*)",
          "metrics": [
            {
              "name": "METRIC_INSTANCE"
            },
            {
              "name": "NSTATE",
              "type": "NORMAL"
            },
            {
              "name": "STIME",
              "type": "NORMAL"
            },
            {
              "name": "CTID",
              "type": "NORMAL"
            },
            {
               "name": "FMRI",
               "type": "NORMAL"
            }
          ]
        }
      ]
    },
    {
      "eventType": "LDMMemstat",
      "command": "bash ./scripts/getLdmMemStat.sh",
      "mappings": [
        {
          "expression": "DOMAIN\\|name=([A-Za-z0-9]+)\\|state=([a-zA-Z]+)\\|flags=([-A-Za-z0-9]+)\\|cons=([a-zA-Z0-9]*)\\|ncpu=([0-9]+)\\|mem=([0-9]+)\\|util=([\\d.]*)\\|uptime=([\\d.]*)\\|norm_util=([\\d.]*)",
          "metrics": [
            {
              "name": "METRIC_INSTANCE"
            },
            {
              "name": "State",
              "type": "NORMAL"
            },
            {
              "name": "Flags",
              "type": "NORMAL"
            },
            {
              "name": "Cons",
              "type": "NORMAL"
            },{
              "name": "Ncpu",
              "type": "NORMAL"
            },
            {
              "name": "Memory",
              "type": "NORMAL"
            },
            {
              "name": "Util",
              "type": "NORMAL"
            },
            {
              "name": "Uptime",
              "type": "NORMAL"
            },
            {
              "name": "Norm Util",
              "type": "NORMAL"
            }
          ]
        }
      ]
    },
	{
		"eventType": "UnixFaults",
		"command": "bash ./scripts/getUnixFaults.sh",
		"mappings": [
		  {
			"expression": "([A-Za-z0-9: ]+)\\s+([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12})\\s+([A-Za-z0-9-]+)\\s+([A-Za-z0-9]+)",
			"metrics": [
			  {
				"name": "METRIC_INSTANCE",
				"type": "DATE"
			  },
			  {
				"name": "EventID",
				"type": "NORMAL"
			  },
			  {
				"name": "MsgID",
				"type": "NORMAL"
			  },
			  {
				"name": "Severity",
				"type": "NORMAL"
				
			  }
			]
		  }
		]
	  },
	  {
		  "eventType": "UnixScriptStats",
		  "command": "cat /tmp/script_status.out && rm /tmp/script_status.out",
		  "mappings": [
			{
			  "expression": "status=([\\d]+)\\scommand=([a-zA-Z.0-9]+)\\stime=([a-zA-Z.0-9:\\s]+)",
			  "metrics": [
				{
				  "name": "StausCode",
				  "type": "NORMAL"
				},
				{
				  "name": "PROCESS_NAME"
				},
				{
					"name": "ExecutedAt",
					"type": "NORMAL"
				}
			  ]
			}
		  ]
		}
    
  ]
}
